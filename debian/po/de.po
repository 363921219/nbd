#
#. Type: string
#. Description
#: ../nbd-server.templates:4001
#, fuzzy
#| msgid "File to export (server number ${number}):"
msgid "Name of export ${number}:"
msgstr "Datei zum Exportieren (Server Nummer ${number}):"

#. Type: string
#. Description
#: ../nbd-server.templates:4001
